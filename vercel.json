{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(index.html|login.html)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/static/$1"
    }
  ]
}