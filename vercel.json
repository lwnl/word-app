{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/",
      "dest": "/static/login.html" 
    },
    {
      "src": "/(.*)",
      "dest": "/static/$1"
    }
  ]
}